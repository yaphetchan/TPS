<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:color="@color/qrom_black" />
    <item android:state_focused="true" android:color="@color/qrom_white" />
    <item android:state_pressed="true" android:color="@color/qrom_white" />
    <item android:state_selected="true" android:color="@color/qrom_white" />
    <item android:color="@color/qrom_blue" android:state_activated="true" />
    <item android:state_checked="true" android:color="@color/qrom_blue" />
    <item android:color="@color/qrom_black" />
</selector>